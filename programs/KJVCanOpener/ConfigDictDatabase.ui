 <class>CConfigDictDatabase</class>
 <widget class="QWidget" name="CConfigDictDatabase">
  <property name="windowTitle">
   <string>Dictionary Database</string>
  </property>
  <layout class="QGridLayout" name="gridLayout">
   <property name="bottomMargin">
    <number>0</number>
   </property>
   <item row="0" column="0">
    <layout class="QVBoxLayout" name="verticalLayout">
     <item>
      <widget class="QLabel" name="lblDictDatabases">
       <property name="text">
        <string>&amp;Dictionary Databases:</string>
       </property>
       <property name="buddy">
        <cstring>treeDictDatabases</cstring>
       </property>
      </widget>
     </item>
     <item>
      <widget class="QTreeView" name="treeDictDatabases">
       <property name="sizePolicy">
        <sizepolicy hsizetype="Expanding" vsizetype="MinimumExpanding">
         <horstretch>10</horstretch>
         <verstretch>0</verstretch>
        </sizepolicy>
       </property>
       <property name="toolTip">
        <string>List of available Dictionary Databases</string>
       </property>
       <property name="editTriggers">
        <set>QAbstractItemView::NoEditTriggers</set>
       </property>
       <property name="rootIsDecorated">
        <bool>false</bool>
       </property>
       <attribute name="headerVisible">
        <bool>false</bool>
       </attribute>
      </widget>
     </item>
     <item>
      <widget class="QPushButton" name="buttonDisplayDictInfo">
       <property name="text">
        <string>Display Dictionary &amp;Information</string>
       </property>
      </widget>
     </item>
     <item>
      <widget class="QLabel" name="lblMainDictDatabaseSelect">
       <property name="text">
        <string>I&amp;nitial Dictionary Database:</string>
       </property>
       <property name="buddy">
        <cstring>comboBoxMainDictDatabaseSelect</cstring>
       </property>
      </widget>
     </item>
     <item>
      <widget class="QComboBox" name="comboBoxMainDictDatabaseSelect">
       <property name="toolTip">
        <string>Select Main Dictionary Database to Load</string>
       </property>
       <property name="insertPolicy">
        <enum>QComboBox::NoInsert</enum>
       </property>
      </widget>
     </item>
    </layout>
   </item>
   <item row="0" column="1">
    <spacer name="horizontalSpacer">
     <property name="orientation">
      <enum>Qt::Horizontal</enum>
     </property>
     <property name="sizeHint" stdset="0">
      <size>
       <width>0</width>
       <height>20</height>
      </size>
     </property>
    </spacer>
   </item>
  </layout>
 </widget>
 <tabstops>
  <tabstop>treeDictDatabases</tabstop>
  <tabstop>buttonDisplayDictInfo</tabstop>
  <tabstop>comboBoxMainDictDatabaseSelect</tabstop>
 </tabstops>
