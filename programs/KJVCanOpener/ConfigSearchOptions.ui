 <class>CConfigSearchOptions</class>
 <widget class="QWidget" name="CConfigSearchOptions">
  <property name="windowTitle">
   <string>Search Options</string>
  </property>
  <layout class="QGridLayout" name="gridLayout">
   <item row="0" column="0">
    <layout class="QHBoxLayout" name="horizontalLayout">
     <item>
      <widget class="QLabel" name="lblSearchPhraseCompleterMode">
       <property name="text">
        <string>Search Phrase &amp;Completer Mode:</string>
       </property>
       <property name="alignment">
        <set>Qt::AlignRight|Qt::AlignTrailing|Qt::AlignVCenter</set>
       </property>
       <property name="buddy">
        <cstring>comboSearchPhraseCompleterMode</cstring>
       </property>
      </widget>
     </item>
     <item>
      <widget class="QComboBox" name="comboSearchPhraseCompleterMode">
       <property name="sizePolicy">
        <sizepolicy hsizetype="MinimumExpanding" vsizetype="Fixed">
         <horstretch>0</horstretch>
         <verstretch>0</verstretch>
        </sizepolicy>
       </property>
       <property name="sizeAdjustPolicy">
        <enum>QComboBox::AdjustToContents</enum>
       </property>
      </widget>
     </item>
    </layout>
   </item>
   <item row="1" column="0">
    <layout class="QHBoxLayout" name="horizontalLayout_2">
     <item>
      <widget class="QLabel" name="lblSearchPhraseActivationDelay">
       <property name="text">
        <string>Search &amp;Phrase Activation Delay (ms):</string>
       </property>
       <property name="buddy">
        <cstring>spinSearchPhraseActivationDelay</cstring>
       </property>
      </widget>
     </item>
     <item>
      <widget class="CSpinBox" name="spinSearchPhraseActivationDelay">
       <property name="minimum">
        <number>50</number>
       </property>
       <property name="maximum">
        <number>2000</number>
       </property>
       <property name="singleStep">
        <number>50</number>
       </property>
       <property name="value">
        <number>500</number>
       </property>
      </widget>
     </item>
    </layout>
   </item>
   <item row="2" column="0">
    <layout class="QHBoxLayout" name="horizontalLayout_4">
     <item>
      <widget class="QLabel" name="lblAutoCompleterActivationDelay">
       <property name="text">
        <string>Auto Completer Activation &amp;Delay (ms):</string>
       </property>
       <property name="buddy">
        <cstring>spinAutoCompleterActivationDelay</cstring>
       </property>
      </widget>
     </item>
     <item>
      <widget class="CSpinBox" name="spinAutoCompleterActivationDelay">
       <property name="maximum">
        <number>2000</number>
       </property>
       <property name="singleStep">
        <number>50</number>
       </property>
       <property name="value">
        <number>500</number>
       </property>
      </widget>
     </item>
    </layout>
   </item>
   <item row="3" column="0">
    <layout class="QHBoxLayout" name="horizontalLayout_3">
     <item>
      <widget class="QLabel" name="lblInitialNumberOfSearchPhrases">
       <property name="text">
        <string>&amp;Initial Number of Search Phrases:</string>
       </property>
       <property name="buddy">
        <cstring>spinInitialNumberOfSearchPhrases</cstring>
       </property>
      </widget>
     </item>
     <item>
      <widget class="CSpinBox" name="spinInitialNumberOfSearchPhrases">
       <property name="minimum">
        <number>1</number>
       </property>
       <property name="maximum">
        <number>8</number>
       </property>
      </widget>
     </item>
    </layout>
   </item>
   <item row="4" column="0">
    <widget class="QCheckBox" name="checkBoxHideMatchingPhrasesLists">
     <property name="text">
      <string>Hide &amp;Matching Words/Phrases Lists</string>
     </property>
    </widget>
   </item>
   <item row="5" column="0">
    <widget class="QCheckBox" name="checkBoxAutoExpandSearchResultsTree">
     <property name="text">
      <string>&amp;Auto Expand Search Results Tree</string>
     </property>
    </widget>
   </item>
   <item row="6" column="0">
    <widget class="QCheckBox" name="checkBoxHideNotFoundInStatistics">
     <property name="text">
      <string>&amp;Hide &quot;Not Found In&quot; Statistics</string>
     </property>
    </widget>
   </item>
  </layout>
 </widget>
 <customwidgets>
  <customwidget>
   <class>CSpinBox</class>
   <extends>QSpinBox</extends>
   <header>SubControls.h</header>
  </customwidget>
 </customwidgets>
 <tabstops>
  <tabstop>comboSearchPhraseCompleterMode</tabstop>
  <tabstop>spinSearchPhraseActivationDelay</tabstop>
  <tabstop>spinAutoCompleterActivationDelay</tabstop>
  <tabstop>spinInitialNumberOfSearchPhrases</tabstop>
  <tabstop>checkBoxHideMatchingPhrasesLists</tabstop>
  <tabstop>checkBoxAutoExpandSearchResultsTree</tabstop>
  <tabstop>checkBoxHideNotFoundInStatistics</tabstop>
 </tabstops>
